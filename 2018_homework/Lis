#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int main()
{
	int n,a;
	int len = 0;
	vector <int> v;
	vector <int>::iterator it;

	v.push_back(0);
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> a;
		if (v.back() < a)
		{
			v.push_back(a);
			len++;
		}
		else
		{
			it=lower_bound(v.begin(), v.end(), a);
			*it = a;
		}
	}
	cout << len;
}
